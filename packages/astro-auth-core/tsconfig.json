{
  "compilerOptions": {
    "target": "es6",
    "module": "es2020",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "declaration": true,
    "declarationDir": "types",
    "sourceMap": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
